"LightmappedGeneric" 

{ 
   "$basetexture" "custom/tuscany/water_fountain"
    "%keywords" "tf"
	
	"%tooltexture" "dev/tfwater_normal"
	"$translucent" "1"

	"$envmap" "env_cubemap"	

	"$bumpmap" "moon\mercury\mercury1_normal"

	"$surfaceprop" "water"
	"$color" "[ .7 .7 .7 ]"
	"$bumpframe" "0"
	
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
	}
	
}